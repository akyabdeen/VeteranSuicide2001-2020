calculate_summary <- function(data) {
# Compute the summary statistics
n <- length(data$values)
mean_value <- mean(data$values)
sd_value <- sd(data$values)
median_value <- median(data$values)
min_value <- min(data$values)
max_value <- max(data$values)
range_value <- range(data$values)[2] - range(data$values)[1]
skewness_value <- skewness(data$values)
kurtosis_value <- kurtosis(data$values)
se_value <- sd_value / sqrt(n)
summary_df <- data.frame(ems = "values",
n = n,
mean = mean_value,
sd = sd_value,
median = median_value,
min = min_value,
max = max_value,
range = range_value,
skewness = skewness_value,
kurtosis = kurtosis_value,
se = se_value)
print(summary_df)
}
